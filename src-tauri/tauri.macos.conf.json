{
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "os": {
        "all": true
      },
      "fs": {
        "all": false,
        "readFile": true
      },
      "dialog": {
        "all": false,
        "open": true,
        "save": true,
        "message": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "minimize": true,
        "maximize": true,
        "startDragging": true
      }
    },
    "windows": [
      {
